<h2>ジャンル一覧・追加 </h2>

<h4>ジャンル名</h4>
<%= form_with model: @genre, url: admin_genres_path  do |f| %>
 <%= f.text_field :name %>
 <%= f.submit "新規登録" %>
<% end %>


<div>
<table>
  <thead>
    <th>ジャンル</th>
    <th></th>
  </thead>
   <tbody>
     <% @genres.each do |genre| %>
     <tr>
       <td><%= genre.name %></td>
       <td><%= link_to "編集する", edit_admin_genre_path(genre) %></td>
     </tr>
      <% end %>
   </tbody>
</table>
</div>